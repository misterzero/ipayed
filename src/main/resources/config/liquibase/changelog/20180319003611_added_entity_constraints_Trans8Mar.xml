        <addForeignKeyConstraint baseColumnNames="trans_type_code_id"
                                 baseTableName="trans_8_mar"
                                 constraintName="fk_trans8mar_trans_type_code_id"
                                 referencedColumnNames="id"
                                 referencedTableName="transaction_type"/>

    </changeSet>
</databaseChangeLog>
